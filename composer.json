{
    "name": "giorgiosaud/carbonlocalizer",
    "description": "Cambiar idioma a fechas Carbon en idioma humano",
    "require": {
      "laravel/framework": "~5.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jorge Saud",
            "email": "jorgelsaud@gmail.com"
        }
    ],
    "autoload": {
    "psr-4": {
      "Giorgiosaud\\Carbonlocalizer\\":"src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Giorgiosaud\\Carbonlocalizer\\Provider\\CarbonlocalizerServiceProvider"
      ],
      "aliases":{
          "CarbonLoc":"Giorgiosaud\\Carbonlocalizer\\Facades\\Carbonlocalizer"
      }
    }
  },
    "minimum-stability": "dev"
}
